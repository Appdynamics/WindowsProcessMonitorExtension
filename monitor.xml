<monitor>
    <name>AppDynamics Windows Process Monitor</name>
       <type>managed</type>
        <enabled>true</enabled>
        <description>Monitor Windows Process Status</description>
        <monitor-configuration></monitor-configuration>
        <monitor-run-task>
        <execution-style>periodic</execution-style> 
         <execution-frequency-in-seconds>60</execution-frequency-in-seconds> 
         <execution-timeout-in-secs>600</execution-timeout-in-secs>
           <name>AppDynamicsWindowsProcessMonitor</name>
           <type>executable</type>
           <task-arguments>
          </task-arguments>
            <executable-task>
                <type>file</type>
                 <file os-type="windows">run.bat</file>
           </executable-task>
         </monitor-run-task>
</monitor>